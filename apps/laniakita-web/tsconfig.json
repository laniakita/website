{
  "extends": "@ahiakea/typescript-config/nextjs.json",
  "compilerOptions": {
    "lib": ["DOM", "DOM.Iterable", "esnext", "webworker"],
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": ["@serwist/next/typings", "bun"]
  },
  "include": [
    "next-env.d.ts",
    "./**/*.ts",
    "./**/*.tsx",
    "./**/*.js",
    "./**/*.cjs",
    "./**/*.mjs",
    ".next/types/**/*.ts",
    ],
  "exclude": ["node_modules", "public/sw.js"]
}
