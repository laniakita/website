{
  "extends": ["//"],
  "tasks": {
    "prebuild": {
      "inputs": ["$TURBO_DEFAULT$", "content/**"],
      "outputs": [".contentlayer", ".contentlayerhtml", ".contentlayermini", ".versionvault", "public/assets/images/**"]
    },
    "test": {
      "dependsOn": []
    },
    "build": {
      "dependsOn": ["test", "^build"],
      "outputs": [".next/**", "public/dist/**", "public/sw.js"],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env",
        ".env.local",
        ".env.development",
        ".env.production",
        "next.config.*",
        "content/**",
        "public/**"
      ]
    },
    "analyze": {
      "env": ["ANALYZE"],
      "outputs": [],
      "inputs": ["$TURBO_DEFAULT$"],
      "dependsOn": ["build"]
    },
    "build:open-next": {
      "dependsOn": ["build"],
      "env": ["OPEN_NEXT_VERSION", "NEXT_PUBLIC_DEPLOYED_URL"],
      "outputs": [".open-next/**"],
      "inputs": ["$TURBO_DEFAULT$", ".open-next.config.ts"]
    },
    "deploy:production": {
      "dependsOn": ["build:open-next"]
    },
    "copy-sharp": {
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env",
        ".env.local",
        ".env.development",
        ".env.production",
        ".open-next/**",
        "content/**"
      ],
      "outputs": [".open-next/server-functions/default/**"],
      "dependsOn": ["build:open-next"]
    },
    "pretty": {},
    "dev": {},
    "start": {}
  }
}
