{
  "extends": ["//"],
  "globalDotEnv": [".env"],
  "pipeline": {
    "analyze": {
      "env": ["ANALYZE"]
    },
    "rss-gen": {},
    "build": {
      "dotEnv": [".env.production.local", ".env.production", ".env.local", ".env"],
      "outputs": [".next/**", "!.next/cache/**", "public/dist/**"],
      "dependsOn": ["^build", "rss-gen"]
    },
    "dev": {
      "dotEnv": [".env.development.local", ".env.development", ".env.local", ".env"]
    },
    "start": {}
  }
}
