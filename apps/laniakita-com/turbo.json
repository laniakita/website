{
  "extends": ["//"],
  "globalDotEnv": [".env"],
  "pipeline": {
    "analyze": {
      "env": ["ANALYZE"]
    },
    "test": {
      "dependsOn": ["lint"]
    },
    "db-gen": {
      "dependsOn": ["test"]
    },
    "rss-gen": {
      "dependsOn": ["db-gen"]
    },
    "build": {
      "dotEnv": [".env.production.local", ".env.production", ".env.local", ".env"],
      "outputs": [".next/**", "!.next/cache/**", "public/dist/**", "public/sw.js"],
      "dependsOn": ["^build", "db-gen", "rss-gen"]
    },
    "dev": {
      "dotEnv": [".env.development.local", ".env.development", ".env.local", ".env"]
    },
    "start": {}
  }
}
