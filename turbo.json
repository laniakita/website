{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"]
    },
    "test": {},
    "test:watch": {
      "cache": false
    },
    "lint": {},
    "deploy": {
      "dependsOn": ["build", "test", "lint"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
